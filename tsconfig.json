{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "noImplicitReturns": true,
    "outDir": "view/assets/javascript/",
    "pretty": true,
    "removeComments": false,
    "watch": true
  },
  "exclude": [ "node_modules" ],
  "include": [ "view/assets/typescript/*.ts" ]
}
